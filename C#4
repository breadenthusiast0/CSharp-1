using System;

class Program {
  public static void Main (string[] args) {
    
    Console.WriteLine("Escriba un numero de dos digitos: ");
    string input1 = Console.ReadLine();
    int numero1 = Convert.ToInt32(input1);

    Console.WriteLine("Escriba otro numero de dos digitos: ");
    string input2 = Console.ReadLine();
    int numero2 = Convert.ToInt32(input2);

    int suma = numero1 + numero2;

    if (suma % 2 == 0) {
        Console.WriteLine("La suma es par");
    } else {
        Console.WriteLine("La suma es impar");
    }
  }
}
